#fields_under_root: false\par
\par
# Internal queue configuration for buffering events to be published.\par
#queue:\par
  # Queue type by name (default 'mem')\par
  # The memory queue will present all available events (up to the outputs\par
  # bulk_max_size) to the output, the moment the output is ready to server\par
  # another batch of events.\par
  #mem:\par
    # Max number of events the queue can buffer.\par
    #events: 4096\par
\par
    # Hints the minimum number of events stored in the queue,\par
    # before providing a batch of events to the outputs.\par
    # The default value is set to 2048.\par
    # A value of 0 ensures events are immediately available\par
    # to be sent to the outputs.\par
    #flush.min_events: 2048\par
\par
    # Maximum duration after which events are available to the outputs,\par
    # if the number of events stored in the queue is < `flush.min_events`.\par
    #flush.timeout: 1s\par
\par
  # The spool queue will store events in a local spool file, before\par
  # forwarding the events to the outputs.\par
  #\par
  # Beta: spooling to disk is currently a beta feature. Use with care.\par
  #\par
  # The spool file is a circular buffer, which blocks once the file/buffer is full.\par
  # Events are put into a write buffer and flushed once the write buffer\par
  # is full or the flush_timeout is triggered.\par
  # Once ACKed by the output, events are removed immediately from the queue,\par
  # making space for new events to be persisted.\par
  #spool:\par
    # The file namespace configures the file path and the file creation settings.\par
    # Once the file exists, the `size`, `page_size` and `prealloc` settings\par
    # will have no more effect.\par
    #file:\par
      # Location of spool file. The default value is $\{path.data\}/spool.dat.\par
      #path: "$\{path.data\}/spool.dat"\par
\par
      # Configure file permissions if file is created. The default value is 0600.\par
      #permissions: 0600\par
\par
      # File size hint. The spool blocks, once this limit is reached. The default value is 100 MiB.\par
      #size: 100MiB\par
\par
      # The files page size. A file is split into multiple pages of the same size. The default value is 4KiB.\par
      #page_size: 4KiB\par
\par
      # If prealloc is set, the required space for the file is reserved using\par
      # truncate. The default value is true.\par
      #prealloc: true\par
\par
    # Spool writer settings\par
    # Events are serialized into a write buffer. The write buffer is flushed if:\par
    # - The buffer limit has been reached.\par
    # - The configured limit of buffered events is reached.\par
    # - The flush timeout is triggered.\par
    #write:\par
      # Sets the write buffer size.\par
      #buffer_size: 1MiB\par
\par
      # Maximum duration after which events are flushed if the write buffer\par
      # is not full yet. The default value is 1s.\par
      #flush.timeout: 1s\par
\par
      # Number of maximum buffered events. The write buffer is flushed once the\par
      # limit is reached.\par
      #flush.events: 16384\par
\par
      # Configure the on-disk event encoding. The encoding can be changed\par
      # between restarts.\par
      # Valid encodings are: json, ubjson, and cbor.\par
      #codec: cbor\par
    #read:\par
      # Reader flush timeout, waiting for more events to become available, so\par
      # to fill a complete batch as required by the outputs.\par
      # If flush_timeout is 0, all available events are forwarded to the\par
      # outputs immediately.\par
      # The default value is 0s.\par
      #flush.timeout: 0s\par
\par
